{
  "name": "devopsick",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "preinst": "npm install -g bun && yarn install && bun install --yarn",
    "clean": "rm -rf dist && echo 'Done.'",
    "devs": "npx tinacms dev -c \"astro dev --port 8080\"",
    "build": "tinacms build && astro build",
    "preview": "astro preview",
    "postbuild": "pagefind --site dist",
    "format:check": "prettier --plugin-search-dir=. --check .",
    "formats": "prettier --plugin-search-dir=. --write .",
    "syncyarn": "bun bun.lockb",
    "dev": "astro dev --watch",
    "start": "astro check --watch & astro dev --verbose",
    "format": "prettier -w . --cache",
    "astro": "astro",
    "sharp": "cd node_modules/sharp && bun install && cd ../../",
    "lint": "prettier --write  \"**/*.{js,jsx,ts,tsx,md,mdx,astro}\" && eslint --fix \"src/**/*.{js,ts,jsx,tsx,astro}\""
  },
  "dependencies": {
    "@astrojs/check": "^0.5.3",
    "@astrojs/markdown-remark": "^4.2.1",
    "@astrojs/mdx": "^2.1.1",
    "@astrojs/partytown": "^2.0.4",
    "@astrojs/react": "^3.0.10",
    "@astrojs/rss": "^4.0.5",
    "@astrojs/sitemap": "^3.0.5",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/ts-plugin": "^1.5.1",
    "@fontsource/inter": "^5.0.16",
    "@pagefind/default-ui": "^1.0.3",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@rollup/plugin-yaml": "^4.1.2",
    "@tailwindcss/typography": "0.5.10",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "astro": "^4.3.6",
    "astro-expressive-code": "latest",
    "astro-icon": "^0.8.2",
    "balance-text": "^3.3.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "embla-carousel-react": "^8.0.0-rc22",
    "framer-motion": "^11.0.3",
    "lint-staged": "15.2.2",
    "lucide-react": "^0.330.0",
    "million": "^3.0.3",
    "motion": "^10.16.4",
    "pagefind": "^1.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "remark": "^15.0.1",
    "sharp": "^0.32.5",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tsm": "^2.3.0",
    "typescript": "^5.3.3"
  },
  "overrides": {
    "astro-expressive-code": "latest"
  },
  "lint-staged": {
    "*.{astro,js,jsx,ts,tsx,md,mdx,json}": [
      "prettier --write --plugin-search-dir=."
    ]
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/parser": "^7.0.1",
    "@types/node": "latest",
    "eslint": "^8.56.0",
    "eslint-plugin-astro": "0.31.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "prettier": "^3.2.4",
    "bun-types": "^1.0.26",
    "unist-util-visit": "^5.0.0",
    "prettier-config-standard": "^7.0.0",
    "prettier-plugin-astro": "0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-to-markdown": "^2.1.0",
    "mdast-util-to-string": "^4.0.0",
    "mdast-util-toc": "^7.0.0"
  },
  "trustedDependencies": [
    "sharp"
  ]
}
